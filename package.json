{
  "name": "react-webpack-boilerplate-by-adrian",
  "version": "0.1.0",
  "description": "React 16 and Webpack 4 setup with React-Router, MobX state management, i18n localisation, Hot Module Reload, Node Express server, ES6 transpiling, and tools for code quality and testing.",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production --config ./config/webpack.prod.js --color --progress",
    "start": "cross-env NODE_ENV=development node server",
    "start:dev": "npm run start",
    "start:prod": "cross-env NODE_ENV=production node server",
    "test": "eslint app/ || true && cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "keywords": [
    "react",
    "webpack",
    "hmr",
    "react-router",
    "mobx",
    "i18n",
    "babel",
    "eslint",
    "sasslint",
    "jest",
    "istanbul"
  ],
  "author": "Adrian von Gegerfelt <adrian@designbyadrian.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.3",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "friendly-errors-webpack-plugin": "1.7.0",
    "jest": "23.6.0",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.2"
  },
  "dependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "8.1.0",
    "babel-loader": "8.0.2",
    "babel-plugin-react-css-modules": "3.4.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.18",
    "bootstrap": "4.1.3",
    "bytes": "3.0.0",
    "chalk": "2.3.1",
    "compression": "1.7.2",
    "cross-env": "5.1.3",
    "css-loader": "1.0.0",
    "dotenv-webpack": "1.5.5",
    "express": "4.16.2",
    "file-loader": "1.1.11",
    "history": "4.7.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.0.7",
    "i18next": "10.5.0",
    "i18next-browser-languagedetector": "2.1.0",
    "i18next-express-middleware": "1.1.1",
    "i18next-node-fs-backend": "1.0.0",
    "i18next-xhr-backend": "1.5.1",
    "image-webpack-loader": "4.3.1",
    "ip": "1.1.5",
    "isdev": "1.0.1",
    "mini-css-extract-plugin": "0.4.1",
    "minimist": "1.2.0",
    "mobx": "5.1.2",
    "mobx-react": "5.2.8",
    "mobx-react-router": "4.0.5",
    "node-notifier": "5.2.1",
    "node-sass": "4.9.3",
    "postcss-loader": "3.0.0",
    "postcss-sass": "0.3.0",
    "prop-types": "15.6.1",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.11",
    "react-i18next": "7.5.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-switch": "2.3.0",
    "sass-loader": "7.1.0",
    "script-loader": "0.7.2",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "webpack": "4.20.1",
    "webpack-cli": "3.1.1",
    "webpack-merge": "4.1.4",
    "whatwg-fetch": "2.0.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "class-methods-use-this": 0,
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/no-webpack-loader-syntax": 0,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-plusplus": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/no-string-refs": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "require-yield": 0,
      "react/require-default-props": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./config/webpack.prod.js"
        }
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/config/tests/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/tests/mocks/image.js"
    },
    "testRegex": ".*\\.test\\.js$"
  }
}
